    "negotiator": "^1.0.0",
    "next": "15.2.4",
    "next-intl": "^4.0.2",
    "next-themes": "^0.4.6",
    "osu-api-v2-js": "^1.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-tweet": "^3.2.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5"
